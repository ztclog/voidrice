      "shortcut": "F1",
      "refresh": "<Control>r",
      "search": "<ctl>F",
      "quit": "<ctl>Q",
      "all_articles": "<ctl>1",
      "only_unread": "<ctl>2",
      "only_starred": "<ctl>3"
    },
    "article_view": {
      "scroll_up": "k",
      "scroll_down": "j",
      "scrap_content": "r",
      "tag": "m",
      "fullscreen": "f"
    },
    "article_list": {
      "next": "<Shift>j",
      "prev": "<Shift>k",
      "read": "t",
      "mark": "<Control>d",
      "open": "O",
      "copy_url": "<ctl><Shift>C"
    },
    "feed_list": {
      "next": "<Shift>l",
      "prev": "<Shift>h",
      "toggle_expanded": "l",
      "read": "space"
    }
  },
  "share": {
    "pocket_enabled": true,
    "instapaper_enabled": true,
    "twitter_enabled": true,
    "mastodon_enabled": true,
    "reddit_enabled": true,
